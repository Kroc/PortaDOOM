{
	"editor.rulers": [
		80
	],
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"editor.tabSize": 8,
	"editor.renderWhitespace": "boundary",
	"files.exclude": {
		"wiki": true,
		"archive/": true,
		"screenshots/": true,
		"build/": true,
		"releases/": true,
		"**/*.wad": true,
		"**/*.WAD": true,
		"**/*.pk3": true,
		"**/*.PK3": true,
		"**/*.epk": true,
		"**/*.EPK": true,
		"**/*.pke": true,
		"**/*.PKE": true,
		"**/*.deh": true,
		"**/*.DEH": true,
		"**/*.bex": true,
		"**/*.BEX": true,
		"**/*.zip": true,
		"**/*.ZIP": true,
		"**/*.7z": true,
		"**/*.7Z": true,
		"**/*.exe": true,
		"**/*.EXE": true,
		"**/*.dll": true,
		"**/*.DLL": true,
		"**/soundfonts/": true,
		"**/fm_banks/": true,
		"**/*.sf2": true,
		"**/*.SF2": true,
		"**/*.?64": true,
		"**/*.html": true,
		"**/*.HTML": true,
		"**/*.htm": true,
		"**/*.HTM": true,
		"**/tranmap.dat": true,
		"**/stdout.txt": true,
		"**/stderr.txt": true,
		"**/CRASHLOG.TXT": true,
		"PortaDOOM/files/ports/*/*.txt": true,
		"PortaDOOM/files/saves/*/": true,
		"PortaDOOM/files/saves/.gitignore": false,
	},
	"spellright.language": [
		"en"
	],
	"spellright.documentTypes": [
		"markdown",
		"latex"
	]
}